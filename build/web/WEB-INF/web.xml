    </servlet>
    <servlet>
        <servlet-name>OtpVeri</servlet-name>
        <servlet-class>OtpVeri</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AppPg</servlet-name>
        <servlet-class>AppPg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FedBk</servlet-name>
